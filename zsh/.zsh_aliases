alias zi='__zoxide_zi "$@"' # override 'zi' from zinit

alias ff='fastfetch'

alias py='python3'
alias vi='nvim '
alias lg='lazygit'


alias dir='eza --color=always --long --git --icons=always --no-time --no-user --no-filesize -F'
alias ls='eza --color=always --icons=always --no-time --no-user --no-filesize -F --no-permissions'
alias tree='eza --color=always --long --git --icons=always --no-time --no-user --no-filesize -F --no-permissions --tree'

alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."

#Run apps (I don't know why but these apps wont run w/o specific tweaks)
alias discord='unset ELECTRON_OZONE_PLATFORM_HINT && ~/Applications/Discord/Discord &'
alias obsidian='unset ELECTRON_OZONE_PLATFORM_HINT && ~/Applications/Obsidian/obsidian'

alias gui='hyprland'
